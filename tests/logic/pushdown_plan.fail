INSERT INTO t (val0, val1, val2) 
SELECT 
    ARRAY[random(), random(), random()]::real[]::vector, 
    ARRAY[random(), random(), random()]::real[]::vector::halfvec,
    ARRAY[random(), random(), random()]::real[]::vector::sparsevec
FROM generate_series(1, 10000);

