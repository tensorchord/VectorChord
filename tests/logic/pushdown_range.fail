CREATE INDEX ON t USING rabbithole (val2 sparsevec_cos_ops)
WITH (options = "[indexing.hnsw]");

# original style
query I
SELECT val2 FROM t WHERE val2 <=> '{0:0.12, 1:0.24, 2:0.36}/3' < 1 ORDER BY val2 <=> '{0:0.12, 1:0.24, 2:0.36}/3';
----
{0:-0.1, 1:0.1, 2:0.1}/3
{0:0.2, 1:-0.2, 2:0.2}/3

# sphere style
query I
SELECT val2 FROM t WHERE val2 <<=>> sphere('{0:0.12, 1:0.24, 2:0.36}/3'::sparsevec, 1) 
ORDER BY val2 <=> '{0:0.12, 1:0.24, 2:0.36}/3';
----
{0:-0.1, 1:0.1, 2:0.1}/3
{0:0.2, 1:-0.2, 2:0.2}/3

